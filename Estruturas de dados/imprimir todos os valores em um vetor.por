programa {
	funcao inicio() {
		inteiro numeros[] = {39, 1, 10, 459}
		
		// para inteiro chamado posição, iniciado emzero, enqaunto a posição for menor a 3, vou fazer mais mais
		// escreva numeros na posição, vai passar 0 1, 2 e 3
		// estamos exibindo todos os valores da estrutura vetor
		para (inteiro posicao = 0; posicao<=3; posicao ++){
		    escreva (numeros[posicao], "")
		}
	}
}
